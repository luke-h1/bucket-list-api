{
  "name": "bucket-list-api",
  "version": "1.0.0",
  "scripts": {
    "watch": "tsc -w",
    "start": "nodemon build/app",
    "build": "tsc",
    "tsc": "tsc --noEmit",
    "test": "NODE_ENV=test jest",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "format:check": "prettier --list-different \"src/**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "check:node": "check-node-version --node 16 --npm 8",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:format": "prisma format",
    "prisma:migrate-new": "prisma migrate dev --name <NAME>",
    "gen-env": "gen-env-types .env -o src/types/env.d.ts -e .",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "husky": "^8.0.1",
    "lerna": "^5.6.2",
    "lint-staged": ">=10",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4",
    "check-node-version": "^4.2.1",
    "@prisma/client": "^4.1.0",
    "@prisma/internals": "^4.1.0",
    "@prisma/migrate": "^4.1.0",
    "bcrypt": "^5.0.1",
    "config": "^3.3.8",
    "cookie-parser": "^1.4.6",
    "cors": "2.8.5",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "nodemon": "2.0.20",
    "pino": "^8.6.1",
    "pino-pretty": "^9.1.0",
    "prisma": "^4.1.0",
    "response-time": "^2.3.2",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "tsconfig-paths": "^4.0.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "eslint": "^8.24.0",
    "eslint-plugin-jest": "^27.1.1",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.25.2",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.1",
    "@types/config": "^3.3.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.1.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.175",
    "@types/node": "^18.8.3",
    "@types/pino": "^7.0.5",
    "@types/response-time": "^2.3.5",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "7.4.0",
    "gen-env-types": "^1.3.0",
    "jest": "^29.1.2",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix --quiet"
    ],
    "*.{js,jsx,ts,tsx,css,scss,html}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "defaultScope": "global"
    }
  }
}
